# This is intended to be the same version of log4j that sbt is using.
LOG4J_2X_REV = '2.11.1'

jar_library(jars=[
              jar(org='org.apache.logging.log4j', name='log4j-api', rev=LOG4J_2X_REV),
              jar(org='org.apache.logging.log4j', name='log4j-core', rev=LOG4J_2X_REV),
              jar(org='org.apache.logging.log4j', name='log4j-slf4j-impl', rev=LOG4J_2X_REV),
            ])
